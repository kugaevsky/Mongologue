<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% cache('sitemap') do %>
<% @posts.each do |post| %>
<url><loc><%= post_url(post) %></loc></url>
<% end %>
<% @tags.each do |tag| %>
<url><loc><%= root_url+"q=#{tag.id}" %></loc></url>
<% end %>
<% end %>
</urlset>
